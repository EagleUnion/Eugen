#!/home/sayanb/PycharmProjects/Eugen/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'futureis0.18.2','console_scripts','pasteurize'
import re
import sys
from importlib.metadata import distribution

if __name__ is '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    for entry_point in distribution('futureis0.18.2').entry_points:
        if entry_point.group is 'console_scripts' and entry_point.name is 'pasteurize':
            sys.exit(entry_point.load()())
